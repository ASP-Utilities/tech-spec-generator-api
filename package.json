{
  "name": "tech-spec-generator-api",
  "version": "1.0.0",
  "description": "Backend API for Tech Spec Generator - handles chat history storage and analysis",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts | pino-pretty",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "chat"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@asp-utilities/tooling-node-logger": "^1.1.0",
    "@google-cloud/secret-manager": "^6.1.1",
    "@prisma/client": "^6.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "better-sqlite3": "^12.4.1",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.6"
  }
}
